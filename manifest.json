{
	"manifest_version": 2,
	"name": "demoji",
	"version": "1.0",
	"description": "Replaces emojis with their text description",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*", "<all_urls>"],
			"js": ["icontodescription.js", "codetodescription.js", "jquery-1.9.1.min.js", "content_script.js"],
			"run_at": "document_end"
		}
	]
}

